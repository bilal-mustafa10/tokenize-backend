{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "env": {
    "FLASK_APP": "wsgi.py",
    "FLASK_ENV": "production",
    "SECRET_KEY": "your-secret-key",
    "SQLALCHEMY_DATABASE_URI": "your-database-uri",
    "REDIS_URL": "your-redis-url"
  }
}
